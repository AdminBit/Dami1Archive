package ud6;

public class Modulos {
	
	private String nombre;
	private double nota;
	
	public Modulos(String nombre ,double nota) {
		this.nombre=nombre;
		this.nota=nota;
	}

	private final String getNombre() {
		return nombre;
	}

	private final void setNombre(String nombre) {
		this.nombre = nombre;
	}

	private final double getNota() {
		return nota;
	}

	private final void setNota(double nota) {
		this.nota = nota;
	}

	@Override
	public String toString() {
		return "Modulos [nombre=" + nombre + ", nota=" + nota + "]";
	}

	
	
	
}
