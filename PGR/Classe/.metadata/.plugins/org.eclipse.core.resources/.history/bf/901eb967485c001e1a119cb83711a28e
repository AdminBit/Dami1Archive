		Scanner sc =new Scanner(System.in);

		int limitInfer=0,limiteSuper;
		do {
			System.out.println("EL limite Inferiro : ");
			limitInfer=sc.nextInt();
			
			System.out.println("EL limite Inferiro : ");
			limiteSuper=sc.nextInt();
				
			}
			while(limiteSuper < limitInfer );
			
		int myrang ; 
		int oportunidades;
		int myGuess;
		myrang = (int) ((Math.random() * (limiteSuper - limitInfer)) + limitInfer);
		
		if(myrang <= 10) {
			oportunidades=3;
			System.out.println(" just lock this is my rang" + myrang + " tengo " + oportunidades +" oportunidades");
			System.out.println("Guess the number");
			myGuess=sc.nextInt();
			
		}
		
		
		
		
	}

